{
  "name": "src",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/node": "^20.2.3",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "aws-rum-web": "^1.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-typed": "^1.2.0",
    "typescript": "^5.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn lint && react-scripts build",
    "test": "react-scripts test",
    "lint": "prettier --check \"**/*.?(t|j)s?(x)\" --ignore-path .prettierignore",
    "lintFix": "eslint --fix --ext .ts,.tsx,.js,.jsx . && prettier --check \"**/*.?(t|j)s?(x)\" --ignore-path .prettierignore --write"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.6.0",
    "prettier": "^2.8.7"
  }
}
